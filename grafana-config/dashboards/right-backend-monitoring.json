{
    "id": null,
    "title": "Right Backend 監控",
    "tags": ["right-backend", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "請求總數",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus-uid"
            },
            "expr": "rate(http_requests_total[5m])",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "vis": false,
                "legend": false
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "HTTP 狀態碼分布",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus-uid"
            },
            "expr": "sum(rate(http_requests_total[5m])) by (status)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "響應時間 P95",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus-uid"
            },
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "錯誤率",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus-uid"
            },
            "expr": "sum(rate(http_requests_total{status=~\"4..|5..\"}[5m])) / sum(rate(http_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "認證操作統計",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus-uid"
            },
            "expr": "sum(rate(http_requests_total{endpoint=~\"/auth/.*\"}[5m])) by (endpoint)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
